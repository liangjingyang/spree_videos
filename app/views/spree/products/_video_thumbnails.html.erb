<% if videos.size > 1 %>
    <ul id="video-thumbnails" class="thumbnails list-inline" data-hook>
        <% videos.map(&:youtube_data).each do |video| %>
            <li class='tmb-all tmb-<%= video.id %>'>
                <%= link_to image_tag(video.thumbnail_url, "data-video-id" => video.id, :class => 'thumbnail'), 'Javascript:void(0);', :title => video.title, "data-video-id" => video.id %>
            </li>
        <% end %>
    </ul>
<% end %>
